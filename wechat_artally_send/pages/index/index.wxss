action-sheet-cancel{
  font-size: 14px;
  height: 50px;
  /* line-height: 50px */
}
action-sheet-item{
  height: 44px;
  font-size: 14px;
  padding: 0;
  line-height: 44px
}
.jw-set-Otimes picker{
  width: 325rpx
}
 page{
   padding-bottom: 117rpx
 }

.jw-index-banner{
  width: 100%;
  height: 232rpx;
  display: block
}
.jw-index-body{
  width: 706rpx;
  margin:0 auto;
}
.jw-select-gift-view{
  height: 142rpx;
  background:url(https://pic.forunart.com/artgive/wx/home_gift_bg.png) no-repeat;
  background-size: 100% 100%;
  margin: 20rpx 0 0 0;
  position: relative;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  border-radius: 4rpx
}
/* .jw-select-gift-left-icon{
  height:100%;
  width: 100rpx;
  background: aqua;
  float: left
} */
.jw-select-gift-content,.jw-select-gift-title{
  font-size:24rpx;
  position: absolute;
  bottom: 28rpx;
  left: 252rpx;
  color: #999999;
}
.jw-select-gift-title{
  top:29rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #cbaa7c;
}
.jw-select-gift-right-icon{
  margin-top: 61rpx;
}
.jw-icon-choice{
  height: 20rpx;
  width: 10rpx;
  float: right;
  margin-right: 20rpx
}
.jw-select-type,.jw-set-persons,.jw-set-times{
  width: 100%;
  height: 88rpx;
  border-radius: 4rpx;
  background-color: #ffffff;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  margin-top: 20rpx
}
.jw-gift-options{
  display: inline-block;
  height: 100%;
  line-height: 88rpx;
  font-size: 28rpx;
  color: #111;
  margin-left: 16.8rpx;
  vertical-align: top
}

.jw-oneToone-view{
  height: 100%;
  float: right;
}
.jw-oneToone-view-icon{
  height: 39.8rpx;
  width: 39.8rpx;
  margin-top: 24.1rpx;
  margin-left: 20rpx;
  float: left;
  display: block
}
.jw-oneToone-view-text{
  font-size: 24rpx;
  height: 100%;
  display: block;
  line-height: 88rpx;
  margin-right: 10rpx;
  float: right;
  color:#999;
  
}

.jw-one-choice{
  margin-top: 34rpx
}

.jw-set-persons,.jw-set-times{
  margin-top: 20rpx;
  background: none;
  box-shadow: none;

}
.jw-set-Opersons,.jw-set-Gpersons,.jw-set-Otimes{
  width: 345rpx;
  display: block;
  background: #fff;
  height: 100%;
  border-radius: 4rpx;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
  float: right;
}
.jw-set-Opersons,.jw-set-Otimes{
   float: left;
}
.jw-sets-title{
  display: inline-block;
  font-size: 28rpx;
  height: 100%;
  line-height: 88rpx;
  float: left;
  margin-left: 20rpx
}
.jw-sets-input{
  float: right;
  height:100%;
  min-height: 0;
  width: 96rpx;
  font-size: 24rpx;
  text-align: right;
  line-height: 88rpx
}
.jw-sets-other{
  float: right;
  font-size: 28rpx;
  height: 88rpx;
  line-height: 88rpx;
  color: #111;
  margin:0 20rpx 0 10rpx
}
.jw-select-times-icon{
  width: 10rpx;
  height: 20rpx;
  transform: rotate(90deg);
  float: right;
  margin-top: 34rpx;
  margin-right: 20rpx
}
.jw-bless-view{
  height: 44rpx;
  margin-top: 15rpx
}
.jw-bless-hint{
  font-size: 24rpx;
  color:#111;
  height: 44rpx;
  line-height: 44rpx;
  display: inline-block;
  vertical-align: top;
  margin-left: 20rpx;
}
.jw-choose-bless{
  float: right;
  margin-right: 10rpx;
  border-radius: 24.5rpx;
  border: solid 2rpx #cbaa7c;
  background: none;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  color:#cbaa7c;
  width: 158rpx;
  text-align: center
}
.jw-bless-input{
  height: 88rpx;
  background: #fff;
  color: #999;
  font-size: 28rpx;
  padding-left: 20rpx;
  margin-top: 15rpx;
  border-radius: 4rpx;
   box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
}
.jw-money{
  height: 112rpx;
  line-height: 112rpx;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  text-align: center;
  font-weight: 600
}
.jw-money-one{
  font-size: 40rpx;
}
.jw-money-main{
  font-size: 80rpx;
}
.jw-compound-gift{
  width: 580rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background-color: #da0202;
  box-shadow: 0 2rpx 15rpx 0 rgba(218, 2, 2, 0.66);
  color:#fff;
  font-size: 28rpx;
  font-weight: 500;
  line-height: 80rpx;
}
.jw-gifts-view{
  margin-top: 20rpx;
  border-radius: 4rpx;
  background-color: #ffffff;
  box-shadow: 0 0 8rpx 0 rgba(0, 0, 0, 0.1);
}
.jw-gift-lists{
  height: 240rpx;
  background: #fff;
  margin-top: 1rpx
}
.jw-gifts-main-view{
  width: 666rpx;
  margin: 0 auto;
  height:100%;
  border-bottom: solid 1rpx #e9e9e9;
}

.jw-gifts-img-view{
  width: 200rpx;
  height: 200rpx;
  padding-top: 20rpx;
  float: left
}
.jw-gifts-img{
  width: 100%;
  height: 100%;
  display: block;

}
.jw-gifts-msg-view{
  float: right;
  width: 450rpx;
  height: 200rpx;
  margin-top: 20rpx;
}
.jw-gifts-msg-title{
  font-size: 28rpx;
  font-weight: 600;
  color: #111;
  display: block
}
.jw-gifts-msg-alt,.jw-gifts-msg-stock{
  font-size: 22rpx;
  font-weight: 500;
  color: #999;
  display: block;
  margin-top: 8rpx
}
.jw-gifts-msg-univalent{
  font-size: 28rpx;
  font-weight: 600;
  color: #da0202;
  display: block;
  float: left;
  height: 50rpx;
  line-height: 50rpx
}
.jw-gifts-btns-view{
  width: 230rpx;
  height: 50rpx;
  box-sizing: border-box;
  float: right;
  border-radius: 4rpx;
  border: solid 2rpx #e9e9e9;
  
}
.jw-gifts-btns-view text{
  width: 75.8rpx;
  float: left;
  height: 46rpx;
  background: none;
  display: block;
  text-align: center;
  line-height: 46rpx;
  color: #111
}

.jw-gifts-msg-num{
  border-left: 2rpx solid #e9e9e9;
  border-right: 2rpx solid #e9e9e9;
  box-sizing: border-box;
  font-size: 26rpx !important;
  min-height: 0;
  height: 46rpx;
  width: 75.8rpx;
  float: left;
  text-align: center;
  line-height: 46rpx
}
.jw-ishave-view{
  height: 73rpx;
  width: 666rpx;
  margin: 0 auto
}
.jw-total-gifts{
  height: 73rpx;
  line-height: 73rpx;
  font-size: 24rpx;
  color: #9b9b9b;
  letter-spacing: normal;
}
.jw-continue{
  float: right;
  height: 44rpx;
  border-radius: 24.5rpx;
  border: solid 2rpx #cbaa7c;
  margin-top: 12.5rpx;
  font-size: 24rpx;
  line-height: 44rpx;
  width: 182rpx;
  text-align: center;
  color: #cbaa7c
}

.actionLeft{
  float: left;
  margin-left: 42rpx;
}
.actionRight{
  float: right;
  margin-right: 42rpx;
  color: #999;
  font-size: 24rpx
}