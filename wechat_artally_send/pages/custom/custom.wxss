
.jw-custom-banner{
  width: 100%;
  height: 280rpx;
  margin-top: 20rpx;
  display: block
}
.jw-call-custom{
  background: #da0202;
  color: #fff;
  width: 600rpx;
  margin: 30rpx auto 0;
  height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
}
.jw-questions{
  margin-top:20rpx;
  /* height: 500rpx; */
  background: #fff
}
.jw-questions-el{
  height: 160rpx;
  /* background: red; */
  border-bottom: 1rpx solid #f2f2f2
}
.jw-question-left,.jw-question-right{
  height: 100%;
}
.jw-question-left{
  width: 150rpx;
  /* background: red */
}
.jw-question-right{
  width: 520rpx;
  /* background: aqua; */
  font-size: 20rpx;
  height: 100rpx;
  margin-top: 40rpx;
  margin-right: 30rpx
}
.jw-question-icon{
  width: 100rpx;
  height: 100rpx;
  background: #fff;
  margin-left: 20rpx;
  display: block;
  margin-top: 10rpx;
  border-radius: 50%
}
.jw-question-name{
  display: block;
  text-align: center;
  font-size: 26rpx;
  margin-top: 10rpx;
}
.jw-question-bottom{
  height: 60rpx;
  text-align: center;
  font-size: 18rpx;
  vertical-align: bottom;
  line-height: 90rpx
}
.g-callphone{
  width: 72rpx;
	height: 72rpx;
  border-radius: 50%;
	background: #fff url('https://pic.forunart.com/artgive/wx/personal_icon_call.png') no-repeat center;
  background-size: cover;
	box-shadow: 0 0 10rpx 0 rgba(0, 0, 0, 0.2);
  position: fixed;
  bottom: 20rpx;
  right: 30rpx;
  z-index:2;
}
