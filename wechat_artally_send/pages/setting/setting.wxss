/* pages/setting/setting.wxss */
.lq-setting{
  background: #fff;
}
.lq-setting .s-li{
  line-height: 90rpx;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #e9e9e9;
  font-size: 28rpx;
	color: #111111;
}
.lq-setting .s-li icon{
  width: 10rpx;
  height: 20rpx;
  margin-top: 35rpx;
  margin-left: 10rpx;
  float: right;
  background: url('https://pic.forunart.com/artgive/wx/all_icon_choice.png') no-repeat center;
  background-size: cover;
}
.lq-setting .s-nobind{
  float: right;
  font-size: 24rpx;
	color: #999999;
}



